    port: 8080
    datasource:
       driver-class-name: com.mysql.cj.jdbc.Driver
       url: jdbc:mysql://localhost:3306/products
       username: root
       password: root
    jpa:
      database: MYSQL
      show-sql: true
      hibernate:
         ddl-auto: create
         dialect: org.hibernate.dialect.MYSQL5Dialect
         naming_strategy: org.hibernate.cfg.ImprovedNamingStrategy